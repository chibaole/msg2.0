<import src="navbar$7457f0be" />
<import src="diago$66d9fee3" />
<template name="project$67981f42">
  <view class="_div data-v-a0735dbe container">
    <template data="{{...$root[$kk+'0'], $root}}" is="navbar$7457f0be"></template>
    <view class="_div data-v-a0735dbe pic-info">
      <view class="_div data-v-a0735dbe pic">
        <image src="{{ host + boon.title_image_url}}" alt class="_img data-v-a0735dbe"></image>
      </view>
      <view class="_div data-v-a0735dbe prj-info">
        <view class="_p data-v-a0735dbe prj-name">{{boon.description}}</view>
        <view class="_p data-v-a0735dbe prj-little-info">{{boon.lottery_conditions}}
          <label class="_span data-v-a0735dbe hasJoined">已有
            <label style="color: #ff7f4f" class="_span data-v-a0735dbe">{{boon.num_of_participants}}</label>人参与</label>
        </view>
      </view>
    </view>
    <view class="_div data-v-a0735dbe sponsors">
      <view class="_p data-v-a0735dbe sponsors-info">{{boon.sponsor.description}}</view>
      <navigator class="_navigator data-v-a0735dbe switchGoAnchor" target="miniProgram" open-type="navigate" app-id="{{boon.sponsor.app_id}}" path="{{boon.sponsor.app_path}}" extra-data version="release">
        <image class="_img data-v-a0735dbe logo" src="{{sesson_url}}" alt></image>{{boon.sponsor.name}}
        <image class="_img data-v-a0735dbe right_ico" src="../../../static/img/right.png" alt></image>
      </navigator>
    </view>
    <view class="_div data-v-a0735dbe process-prize" wx:if="{{boon.status== 'published'}}">
      <view class="_h2 data-v-a0735dbe">抽奖流程</view>
      <view class="_div data-v-a0735dbe steps">
        <view class="_p data-v-a0735dbe step1">1.点击抽奖，等待开奖</view>
        <view class="_p data-v-a0735dbe step2">2.领取成功后，请扫码加群等待发货哦</view>
      </view>
      <view class="_div data-v-a0735dbe line"></view>
      <view class="_h2 data-v-a0735dbe">赞助商介绍</view>
      <view class="_div data-v-a0735dbe steps">
        <view class="_p data-v-a0735dbe step1">1.点击抽奖，等待开奖</view>
        <view class="_p data-v-a0735dbe step2">2.领取成功后，请扫码加群等待发货哦</view>
      </view>
    </view>
    <view class="_div data-v-a0735dbe btn1" wx:if="{{boon.status== 'published'}}">
      <button wx:if="{{boon.participate_status == true}}" class="_button data-v-a0735dbe waiting">待开奖</button>
      <button bindtap="handleProxy" wx:if="{{boon.participate_status == false}}" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_button data-v-a0735dbe {{prizeStyle}}">{{prize}}</button>
    </view>
    <view class="_div data-v-a0735dbe openPrize" wx:if="{{boon.status== 'rewarded'}}">
      <view class="_div data-v-a0735dbe pic">
        <image src="http://pbmrxkahq.bkt.clouddn.com/winning.png" alt wx:if="{{priceResult.win == true}}" class="_img data-v-a0735dbe"></image>
        <image src="http://pbmrxkahq.bkt.clouddn.com/%E6%9C%AA%E4%B8%AD%E5%A5%96.png" alt wx:if="{{priceResult.win == false}}" class="_img data-v-a0735dbe"></image>
        <view class="_p data-v-a0735dbe">{{priceResult.res}}</view>
      </view>
      <view class="_div data-v-a0735dbe prizeWindow" wx:if="{{priceResult.win == true}}" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}">去领奖</view>
      <view class="_div data-v-a0735dbe nameList">
        <view class="_div data-v-a0735dbe line1"></view>
        <view class="_div data-v-a0735dbe line2"></view>
        <view class="_div data-v-a0735dbe title">中奖者名单</view>
      </view>
      <view class="_div data-v-a0735dbe userBox">
        <view class="_div data-v-a0735dbe user">
          <image src="http://p15hnzxrp.bkt.clouddn.com/tuzi2.jpeg" alt class="_img data-v-a0735dbe"></image>
          <view class="_div data-v-a0735dbe nickname">{{}}天王盖被子</view>
        </view>
        <view class="_div data-v-a0735dbe getall">
          <label class="_span data-v-a0735dbe">加载全部</label>
          <image src="http://pbmrxkahq.bkt.clouddn.com/%E5%8A%A0%E8%BD%BD%E6%9B%B4%E5%A4%9Aicon.png" alt class="_img data-v-a0735dbe"></image>
        </view>
      </view>
    </view>
    <view class="_div data-v-a0735dbe btn-box" wx:if="{{boon.status== 'published'}}">
      <template wx:if="{{open}}" bindinfo="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" data="{{...$root[$kk+'1'], $root}}" is="diago$66d9fee3"></template>
      <view class="_div data-v-a0735dbe add-prize">
        <view class="_div data-v-a0735dbe btn2" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">加速开奖</view>
        <view class="_div data-v-a0735dbe btn3" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">成为研究员概率翻倍</view>
      </view>
      <view class="_div data-v-a0735dbe mask" wx:if="{{showBox}}">
        <view class="_div data-v-a0735dbe meunBox" wx:if="{{showBox}}">
          <image class="_img data-v-a0735dbe x" src="../../../static/img/close2.png" alt bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}"></image>
          <view class="_div data-v-a0735dbe title">分享加速抽奖</view>
          <button class="_button data-v-a0735dbe friend" open-type="share">
            <image src="../../../static/img/wechatF.png" alt class="_img data-v-a0735dbe"></image>
          </button>
          <view class="_div data-v-a0735dbe createImg" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">
            <image src="../../../static/img/wechatimg.png" alt class="_img data-v-a0735dbe"></image>
          </view>
          <view class="_div data-v-a0735dbe wechatFriend" open-type="share">微信好友</view>
          <view class="_div data-v-a0735dbe shengchengImg" bindtap="handleProxy" data-eventid="{{'7'}}" data-comkey="{{$k}}">生成分享图片</view>
        </view>
      </view>
    </view>
    <view class="_div data-v-a0735dbe btn-box" wx:if="{{boon.status== 'rewarded'}}">
      <view class="_div data-v-a0735dbe add-prize">
        <view class="_div data-v-a0735dbe btn4" bindtap="handleProxy" data-eventid="{{'8'}}" data-comkey="{{$k}}">再去抽一个</view>
      </view>
    </view>
  </view>
</template>